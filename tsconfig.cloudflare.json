{
  "extends": "./tsconfig.json",
  "include": [
    ".react-router/types/**/*",
    "app/**/*",
    "app/**/.server/**/*",
    "app/**/.client/**/*",
    "workers/**/*",
    "worker-configuration.d.ts",
    "source.config.ts",
    "react-router.config.ts",
    ".source/**/*",
  ],
  "compilerOptions": {
    "composite": true,
    "strict": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ES2022"
    ],
    "types": [
      "vite/client"
    ],
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "baseUrl": ".",
    "rootDirs": [
      ".",
      "./.react-router/types"
    ],
    "paths": {
      "~/*": [
        "./app/*"
      ],
      "content-collections": [
        "./.content-collections/generated"
      ],
      "@docs": [
        ".source"
      ]
    },
    "esModuleInterop": true,
    "resolveJsonModule": true
  }
}